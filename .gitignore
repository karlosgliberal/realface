# Al usar composer no deberíamos meter lo siguiente, pero lo comento porque en Acquia no se puede
# hacer un deploy que no venga de GIT

# Ignore binaries.
# /bin

# Ignore non-custom parts of Drupal.
# /docroot/core
# /docroot/modules/contrib
# /docroot/profiles/contrib
# /docroot/themes/contrib
# /docroot/sites/default/files

# Ignore Composer-managed dependencies.
# vendor/

# Fin de ignores de composer.

*.log

# Ignoramos los ficheros de GIT de vendors
# https://getcomposer.org/doc/faqs/should-i-commit-the-dependencies-in-my-vendor-directory.md

.git

#Ignorar configuración local
/docroot/sites/*/settings.local.php

#Ignorar directorio 'dumps' que genera menu.sh
/docroot/dumps

# Ignore Drupal's file directory
/docroot/files
/docroot/sites/*/files
/docroot/sites/*/private
/acquia-files
/docroot/themes/custom/tagliatella/dist/icons/sprite/sprite.symbol.html


# Ignore sql dumps
*.sql
*.sql.gz

# Ignore files generated by PhpStorm
.idea

# Ignore mysql docker files
#mysql

# Ignore Mac temp files
.DS_Store
._.DS_Store

# Ignore node_modules
node_modules
